<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Booking"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".Activites.MeetingActivity"
            android:exported="false" />
        <activity
            android:name=".Activites.BookingListActivity"
            android:exported="false"
            android:label="@string/title_activity_booking_list"
            android:theme="@style/Theme.Booking" />
        <activity
            android:name=".Activites.DeskWorkActivity"
            android:exported="false"
            android:parentActivityName=".Activites.Home_Screen" />
        <activity
            android:name=".Activites.Home_Screen"
            android:exported="false" />
        <activity
            android:name=".Activites.RegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".Activites.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>